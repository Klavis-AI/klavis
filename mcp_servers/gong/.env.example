# Environment variables
# Copy this file to .env and fill in your actual values

# Gong API credentials (get from Gong API Page - requires technical administrator privileges)
# The server will automatically combine and Base64-encode them as per Gong API requirements
GONG_ACCESS_KEY=your_access_key_here
GONG_ACCESS_KEY_SECRET=your_access_key_secret_here

# Server configuration
GONG_MCP_SERVER_PORT=5000
