# Zapier MCP Server Environment Variables
# Copy this file to .env and fill in your values

# Required: Your Zapier API key
ZAPIER_API_KEY=your_zapier_api_key_here

# Optional: Server configuration
ZAPIER_MCP_SERVER_PORT=5000

# Optional: Logging configuration
LOG_LEVEL=INFO

# Optional: API configuration (if different from defaults)
# API_BASE_URL=https://api.zapier.com/v1
# API_TIMEOUT=30
